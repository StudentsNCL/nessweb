<div class="row">
    {{#each coursework}}
    <div class="col-md-12">
        <h1>{{code}}: {{title}}</h1>
        <table class="table table-condensed">
            <thead>
                <tr>
                    <th>Title</th>
                    <th>Due</th>
                </tr>
            </thead>
            <tbody>
             {{#each this.coursework}}
                <tr>
                    <td width="80%">{{#if url}}<a href="{{url}}">{{title}}</a>{{else}}{{title}}{{/if}}</td>
                    <td title="{{formatDate due}}">{{dueDate due}}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
    {{/each}}
</div>