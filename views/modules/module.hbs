
<h1>Module Overview</h1>
<table class="table table-condensed table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Mark</th>
            <th>Type</th>
        </tr>
    </thead>
    <tbody>
     {{#each module}}
        <tr>
            <td>{{name}}</td>
            <td>{{#if mark}}{{mark}}{{else}}TBR{{/if}}</td>
			<td>{{type}}</td>            
        </tr>
    {{/each}}
    </tbody>
</table>
